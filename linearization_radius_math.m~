clear
close all
clc

%% Linearization
syms x y vel theta u1 u1 real

dx = vel*cos(theta);
dy = vel*sin(theta);

v1 = 0.5*cos(theta)*u1 - 





%% Radial CBF


q = [x; y];
dq = [dx; dy];
v = [v1; v2];
z = [q; dq];
dz = [dq; v];

r = sqrt(x^2 + y^2 + 0.00001);
dr = jacobian(r, q)*dq;

% h = -(r + dr - 1);
h = -(r + 0.5*dr^2 - 1);

f = [zeros(2), eye(2); zeros(2), zeros(2)]*z;
g = [zeros(2); eye(2)]*v;

Lfh = jacobian(h, z)*f;
Lgh = jacobian(h, z)*g;


%% Generate functions

if ~exist('./gen')
    mkdir('./gen')
end
addpath('./gen')

matlabFunction(f, 'File', 'gen/f_gen', 'Vars', {z});
matlabFunction(g, 'File', 'gen/g_gen', 'Vars', {z, v});
matlabFunction(h, 'File', 'gen/h_gen', 'Vars', {z});
matlabFunction(Lfh, 'File', 'gen/Lfh_gen', 'Vars', {z});
matlabFunction(Lgh, 'File', 'gen/Lgh_gen', 'Vars', {z, v});

matlabFunction(h2, 'File', 'gen/h2_gen', 'Vars', {z, ts});
matlabFunction(Lfh2, 'File', 'gen/Lfh2_gen', 'Vars', {z, ts});
matlabFunction(Lgh2, 'File', 'gen/Lgh2_gen', 'Vars', {z, v, ts});
